extends layout

block content
  h1= title
  p Welcome to #{title}
  form(name="buttons")
    input(type="button" name="next" value="next" onClick="nextGene()")
    input(type="button" name="start" value="start" onClick="startAuto()")
    input(type="button" name="stop" value="stop" onClick="stopAuto()")
    input(type="button" name="random" value="random" onClick="repositioningCell()")
    input(type="button" name="up" value="up" onClick="upSpeed()")
    input(type="button" name="down" value="down" onClick="downSpeed()")
    input(type="button" name="clear" value="clear" onClick="cellClear()")
  canvas(id="maincanvas")
